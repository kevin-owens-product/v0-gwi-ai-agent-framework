{
  "name": "gwi-ai-agent-framework",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && npm run db:migrate && next build && npm run db:seed",
    "build:no-seed": "prisma generate && npm run db:migrate && next build",
    "build:with-seed": "prisma generate && npm run db:migrate && next build && npm run db:seed",
    "build:render": "node scripts/render-build.js",
    "dev": "next dev",
    "dev:seed": "npm run db:seed && next dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "next start",
    "postinstall": "prisma generate",
    "db:migrate": "node scripts/db-migrate.mjs",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:minimal": "tsx prisma/seed-minimal.ts",
    "db:seed-if-empty": "tsx prisma/seed-if-empty.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "i18n:validate": "node scripts/validate-translations.js",
    "i18n:validate:verbose": "node scripts/validate-translations.js --verbose",
    "i18n:fix": "node scripts/validate-translations.js --fix",
    "i18n:clean": "node scripts/validate-translations.js --clean",
    "i18n:sync": "node scripts/validate-translations.js --fix --clean",
    "i18n:scan": "node scripts/scan-hardcoded-strings.js",
    "i18n:scan:fix": "node scripts/scan-hardcoded-strings.js --fix",
    "i18n:scan:strict": "node scripts/scan-hardcoded-strings.js --strict",
    "i18n:audit": "npm run i18n:validate && npm run i18n:scan",
    "i18n:audit:strict": "npm run i18n:validate --strict && npm run i18n:scan --strict",
    "ci": "npm run type-check && npm run lint && npm run i18n:validate && npm run test:coverage && npm run build",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@auth/prisma-adapter": "^2.7.4",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "5.22.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@sentry/nextjs": "^10.32.1",
    "@tailwindcss/postcss": "^4.1.9",
    "@tanstack/react-query": "^5.62.2",
    "@types/dompurify": "^3.0.5",
    "@upstash/ratelimit": "^2.0.4",
    "@upstash/redis": "^1.34.3",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "input-otp": "1.4.1",
    "isomorphic-dompurify": "^2.35.0",
    "lucide-react": "^0.454.0",
    "next": "16.0.10",
    "next-auth": "^5.0.0-beta.25",
    "next-intl": "^4.7.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5",
    "react": "19.2.0",
    "react-day-picker": "9.8.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.60.0",
    "react-markdown": "10.1.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.4",
    "resend": "^6.7.0",
    "sonner": "^1.7.4",
    "stripe": "^17.3.1",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.9",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.0",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "vaul": "^1.1.2",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "google-translate-api-x": "^10.7.2",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "prisma": "5.22.0",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0 --no-warn-ignored"
    ]
  }
}
